[Unit]
Description=gunicorn daemon
Requires=gunicorn.socket
After=network.target
[Service]
User=ubuntu
Group=www-data
WorkingDirectory=/home/ubuntu/backend-paysofter
ExecStart=/home/ubuntu/env/bin/gunicorn --access-logfile - --workers 3 --bind unix:/home/ubuntu/backend-paysofter/backend-paysofter/backend-paysofter.sock backend-paysofter.wsgi:application

       
[Install]
WantedBy=multi-user.target
